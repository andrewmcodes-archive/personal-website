{
  "name": "new-bridgetown-site",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://andrewmcodes-v2.netlify.app",
  "bugs": {
    "url": "https://github.com/andrewmcodes/site-v2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:andrewmcodes/site-v2.git"
  },
  "license": "MIT",
  "author": {
    "name": "Andrew Mason",
    "email": "andrewmcodes@protonmail.com"
  },
  "scripts": {
    "build": "bundle exec bridgetown build",
    "clean": "bundle exec bridgetown clean",
    "serve": "bundle exec bridgetown serve",
    "deploy": "yarn webpack-build && yarn build",
    "prettier-check": "prettier --check \"./**/*.{js,json,css,yml}\"",
    "prettier-format": "prettier --write \"./**/*.{js,json,css,yml}\"",
    "snapshot-dev": "yarn percy snapshot output/",
    "start": "node start.js",
    "sync": "node sync.js",
    "webpack-build": "webpack --mode production",
    "webpack-dev": "webpack --mode development -w",
    "webpack-analyze": "mkdir -p output && node_modules/.bin/webpack --profile --json > output/stats.json && node_modules/.bin/webpack-bundle-analyzer output/stats.json"
  },
  "browserslist": [
    "defaults",
    "maintained node versions",
    "> 5%"
  ],
  "dependencies": {
    "@swup/body-class-plugin": "^1.0.2",
    "@swup/scroll-plugin": "^1.0.6",
    "@swup/slide-theme": "^1.0.5",
    "stimulus": "^1.1.1",
    "swup": "^2.0.10",
    "typed.js": "^2.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@tailwindcss/ui": "^0.3.0",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-prismjs": "^2.0.1",
    "browser-sync": "^2.26.7",
    "browserslist": "^4.12.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "percy": "^0.26.9",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-easings": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-import-url": "^5.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "prismjs": "^1.20.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "tailwind-heropatterns": "^0.0.8",
    "tailwindcss": "^1.4.6",
    "tailwindcss-debug-screens": "^0.1.0",
    "tailwindcss-gradients": "^3.0.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "engines": {
    "node": "13.x.x",
    "npm": "6.x.x"
  }
}
